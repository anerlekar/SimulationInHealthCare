<script>
	function calc(){
		var amt1 = document.getElementById('healthinsurance').innerText;
		var amt2 = document.getElementById('pharmacycharges').innerText;
		var amt3 = document.getElementById('doctorcharges').innerText;
		var total = parseInt(amt3) + parseInt(amt2) + parseInt(amt1);
		var totalWithTax = total + (total * 0.08);
		console.log(">>"+totalWithTax);
		document.getElementById('total').innerText = '$' + totalWithTax;
	}
</script>
	
